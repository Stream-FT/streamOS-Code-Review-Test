{
    "name": "streamos",
    "version": "1.0.0",
    "description": "A Node.js project using Express and Jest",
    "main": "dist/index.js",
    "scripts": {
      "start": "node dist/index.js",
      "dev": "ts-node-dev --respawn --transpile-only --require module-alias/register index.ts",
      "build": "tsc",
      "postbuild": "copyfiles -u 1 assets/**/* dist/assets",
      "test": "jest",
      "prepare": "husky && husky install",
      "migration:create": "prisma generate",
      "migration:dev": "prisma migrate dev",
      "migration:reset": "prisma migrate reset",
      "migration:deploy": "prisma migrate deploy",
      "migration:seed": "prisma db seed",
      "start4000": "PORT=4000 node dist/index.js",
      "start:dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
      "debug": "concurrently \"npx tsc --watch\" \"nodemon --inspect -q dist/index.js\"",
      "types:check": "tsc --noEmit",
      "test:seed": "ts-node prisma/seed/test/test.seed.ts",
      "test:seed:clean": "ts-node prisma/seed/test/test-clean.seed.ts",
      "test:unit": "jest --config ./jest-unit.config.ts",
      "test:e2e": "jest --config ./jest-e2e.config.ts"
    },
    "_moduleAliases": {
      "@accounting": "dist/accounting/src",
      "@common": "dist/common/src",
      "@onboarding": "dist/onboarding/src",
      "@authentication": "dist/authentication/src"
    },
    "prisma": {
      "seed": "ts-node --require module-alias/register prisma/seed/index.ts"
    },
    "dependencies": {
      "@knocklabs/node": "^0.4.22",
      "@lumigo/opentelemetry": "^1.41.3",
      "@opentelemetry/api": "^1.9.0",
      "@opentelemetry/instrumentation": "^0.52.1",
      "@opentelemetry/resources": "^1.25.1",
      "@opentelemetry/sdk-trace-base": "^1.25.1",
      "@opentelemetry/tracing": "^0.24.0",
      "@prisma/client": "^5.16.1",
      "@prisma/instrumentation": "^5.16.1",
      "aws-sdk": "^2.1691.0",
      "axios": "^1.7.7",
      "cookie": "0.7.0",
      "cors": "^2.8.5",
      "csv-parse": "^6.1.0",
      "dayjs": "^1.11.13",
      "dotenv": "^16.0.3",
      "eslint-import-resolver-typescript": "^3.6.1",
      "eslint-plugin-check-file": "^2.8.0",
      "express": "^4.20.0",
      "express-validator": "^7.1.0",
      "fuse.js": "^7.0.0",
      "helmet": "^8.0.0",
      "jsonwebtoken": "^9.0.2",
      "micromatch": "4.0.8",
      "module-alias": "^2.2.3",
      "morgan": "^1.10.0",
      "node-cron": "^3.0.3",
      "openapi-types": "^12.1.3",
      "send": "0.19.0",
      "serve-static": "1.16.0",
      "typescript": "5.4.5",
      "winston": "^3.8.2",
      "ws": "8.17.1",
      "zod": "^3.22.3"
    },
    "devDependencies": {
      "@faker-js/faker": "^7.6.0",
      "@types/cors": "^2.8.13",
      "@types/csv-parse": "^1.1.12",
      "@types/express": "^4.17.21",
      "@types/jest": "^29.4.0",
      "@types/jsonwebtoken": "^9.0.6",
      "@types/morgan": "^1.9.9",
      "@types/node": "^20.14.2",
      "@types/node-cron": "^3.0.11",
      "@types/serve-static": "^1.15.7",
      "@types/supertest": "^2.0.12",
      "@types/swagger-jsdoc": "^6.0.4",
      "@types/swagger-ui-express": "^4.1.6",
      "concurrently": "^8.2.2",
      "copyfiles": "^2.4.1",
      "husky": "^8.0.0",
      "jest": "^29.4.1",
      "lint-staged": "^15.2.5",
      "prisma": "^5.16.1",
      "prisma-erd-generator": "^1.11.2",
      "supertest": "^6.3.3",
      "ts-jest": "^29.2.5",
      "ts-node": "^10.9.2",
      "ts-node-dev": "^2.0.0"
    },
    "engines": {
      "node": ">=18"
    },
    "keywords": [],
    "author": "",
    "license": "ISC"
  }
  